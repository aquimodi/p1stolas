{
  "name": "datacenter-management-app",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd server && npm run dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "vite build",
    "build:production": "vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "deploy:windows": "powershell -ExecutionPolicy Bypass -File .\\deploy-windows.ps1",
    "deploy:windows-full": "powershell -ExecutionPolicy Bypass -File .\\deploy-windows.ps1 -InstallNginx -StartServices",
    "nginx:start": "powershell -Command \"Start-Process -FilePath 'D:\\nginx\\nginx.exe' -WorkingDirectory 'D:\\nginx'\"",
    "nginx:stop": "powershell -Command \"D:\\nginx\\nginx.exe -s stop\"",
    "nginx:reload": "powershell -Command \"D:\\nginx\\nginx.exe -s reload\"",
    "nginx:test": "powershell -Command \"D:\\nginx\\nginx.exe -t\"",
    "check:health": "powershell -Command \"Invoke-WebRequest -Uri 'http://localhost/health' -Method GET\"",
    "check:backend": "powershell -Command \"Invoke-WebRequest -Uri 'http://localhost:3002/api/health' -Method GET\"",
    "logs:nginx": "powershell -Command \"Get-Content 'D:\\nginx\\logs\\error.log' -Tail 50\"",
    "logs:access": "powershell -Command \"Get-Content 'D:\\nginx\\logs\\access.log' -Tail 50\"",
    "logs:backend": "powershell -Command \"Get-Content '.\\logs\\backend-combined.log' -Tail 50\"",
    "status:nginx": "powershell -Command \"Get-Process -Name 'nginx' -ErrorAction SilentlyContinue\"",
    "pm2:start": "pm2 start ecosystem.config.cjs --env production",
    "pm2:stop": "pm2 stop datacenter-backend",
    "pm2:restart": "pm2 restart datacenter-backend",
    "pm2:logs": "pm2 logs datacenter-backend --lines 50",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "pm2:delete": "pm2 delete datacenter-backend",
    "backend:install": "cd server && npm install",
    "backend:start": "cd server && npm start",
    "setup:backend": "npm run backend:install && npm run pm2:start"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "mssql": "^10.0.2",
    "dotenv": "^16.4.5",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.2",
    "date-fns": "^3.3.1",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.50.1",
    "react-beautiful-dnd": "^13.1.1",
    "zustand": "^4.5.2",
    "axios": "^1.6.7",
    "xlsx": "^0.18.5",
    "clsx": "^2.1.0",
    "tesseract.js": "^5.0.5",
    "pdfjs-dist": "^4.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-security": "^2.1.1",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^1.4.0",
    "concurrently": "^8.2.2"
  }
}